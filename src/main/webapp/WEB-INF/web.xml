<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>help</servlet-name>
		<servlet-class>org.patsimas.school.controllers.HelpServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>help</servlet-name>
		<url-pattern>/exp</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>org.patsimas.school.controllers.LogoutServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>api_users</servlet-name>
		<servlet-class>org.patsimas.school.api.UserListAPI</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>api_users</servlet-name>
		<url-pattern>/api/users</url-pattern>
	</servlet-mapping>

</web-app>
